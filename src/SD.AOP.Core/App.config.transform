<configuration>
  <configSections>
    <!--SD.AOP配置节点-->
    <section name="sd.aop" type="SD.AOP.Core.AopSection, SD.AOP.Core"/>
  </configSections>
  <!--SD.AOP配置-->
  <sd.aop>
    <!--日志记录者-->
    <loggerProvider type="SD.AOP.Core.DefaultLogger" assembly="SD.AOP.Core" />
    <!--连接字符串-->
    <connectionString name="LogConnection" value="Data Source=.;Initial Catalog=LogDB;User Id=sa;Password=123456;MultipleActiveResultSets=true;" />
  </sd.aop>
</configuration>